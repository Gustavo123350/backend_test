{
  "env": {
    "es2021":true,
    "node": true
  },
    "extends": [
      "eslint:recomended",
      "plugin:@typescript-eslint/recomended"
   ],
   "parser" : "@typescript-eslint/parser",
   "parserOptions" : {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": [
    "@typescript-eslint"
   ],
   "rules": {
      "indent" : [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes":[
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "@typescript-eslint/no-interface":"off"
   }

}